{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfstorage59"
		},
		"AzureBlobStorageUseCase01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageUseCase01'"
		},
		"AzureBlobStorageUseCase02_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageUseCase02'"
		},
		"AzureBlobStorageUseCase03_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageUseCase03'"
		},
		"AzureDataLakeStorageUseCase01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageUseCase01'"
		},
		"AzureDataLakeStorageUseCase02_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageUseCase02'"
		},
		"AzureSqlDatabaseUseCase03_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseUseCase03'"
		},
		"AzureDataLakeStorageUseCase01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vaishnavidatalake56.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageUseCase02_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vaishnavidatalake56.dfs.core.windows.net/"
		},
		"AzureSqlDatabaseUseCase03_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "vaishnavi-server.database.windows.net"
		},
		"AzureSqlDatabaseUseCase03_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "vaishnavi-database"
		},
		"AzureSqlDatabaseUseCase03_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vaishnavi"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfstorage59_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageUseCase01_connectionString": {
						"value": "[parameters('AzureBlobStorageUseCase01_connectionString')]"
					},
					"AzureBlobStorageUseCase02_connectionString": {
						"value": "[parameters('AzureBlobStorageUseCase02_connectionString')]"
					},
					"AzureBlobStorageUseCase03_connectionString": {
						"value": "[parameters('AzureBlobStorageUseCase03_connectionString')]"
					},
					"AzureDataLakeStorageUseCase01_accountKey": {
						"value": "[parameters('AzureDataLakeStorageUseCase01_accountKey')]"
					},
					"AzureDataLakeStorageUseCase02_accountKey": {
						"value": "[parameters('AzureDataLakeStorageUseCase02_accountKey')]"
					},
					"AzureSqlDatabaseUseCase03_password": {
						"value": "[parameters('AzureSqlDatabaseUseCase03_password')]"
					},
					"AzureDataLakeStorageUseCase01_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageUseCase01_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageUseCase02_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageUseCase02_properties_typeProperties_url')]"
					},
					"AzureSqlDatabaseUseCase03_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseUseCase03_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseUseCase03_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseUseCase03_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseUseCase03_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseUseCase03_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}